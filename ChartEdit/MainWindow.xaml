<Window
    x:Class="ChartEdit.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ChartEdit"
    xmlns:localVM="clr-namespace:ChartEdit.ViewModel"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:oxyWpf="http://oxyplot.org/wpf"
    Title="MainWindow"
    Width="800"
    Height="450"
    d:DataContext="{d:DesignInstance Type=localVM:MainWindowVM}"
    mc:Ignorable="d">
    <Window.DataContext>
        <localVM:MainWindowVM />
    </Window.DataContext>
    <Grid />
</Window>
