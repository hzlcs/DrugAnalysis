<Page x:Class="ChartEditWPF.Pages.VerticalIntegralConfigPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChartEditWPF.Pages"
      xmlns:vm="clr-namespace:ChartEditWPF.ViewModels"
      xmlns:hc="https://handyorg.github.io/handycontrol"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="VerticalIntegralConfigPage"
      d:DataContext="{d:DesignInstance vm:VerticalIntegralConfigViewModel}">

    <Grid>
        <hc:PropertyGrid SelectedObject="{Binding Config}"/>
    </Grid>
</Page>
